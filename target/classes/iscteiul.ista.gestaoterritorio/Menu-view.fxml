<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iscteiul.ista.gestaoterritorio.GUIController">
    <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../images/papaiCris.png" />
         </image>
      </ImageView>

        <VBox alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Text style="-fx-font-size: 24px; -fx-font-weight: bold;" text="Menu Principal" />
                <Button fx:id="searchLandButton" onAction="#procurarTerrenoClick" prefWidth="200.0" text="Procurar Terreno" />
                <Button onAction="#vizualizarGrafoClick" prefWidth="200.0" text="Visualizar Grafo de Propriedades" />
                <Button onAction="#calcularAreaClick" prefWidth="200.0" text="Calcular Área Média" />
                <Button onAction="#close" prefWidth="200.0" text="Sair" />
            </children>
        </VBox>
    </children>
</AnchorPane>
